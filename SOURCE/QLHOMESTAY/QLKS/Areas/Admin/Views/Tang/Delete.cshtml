@model QLKS.Models.tblTang

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a href="@Url.Action("Index","Index")">Quản Lýs</a>
    </li>
    <li class="breadcrumb-item">
        <a href="@Url.Action("Index","Tang")">Khu Vực</a>
    </li>
    <li class="breadcrumb-item active">Xóa Khu Vực</li>
</ol>
<div class="card mb-3">
    <div class="card-header">
        <i class="fa fa-table"></i> Delete
    </div>
    <div class="card-body">
        <div class="table-responsive">

            <h3>Bạn có chắc chắn muốn xóa</h3>
            <div>
                <dl class="dl-horizontal">
                    <dt>
                        Khu Vực
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.ten_tang)
                    </dd>

                </dl>

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Xóa" class="btn btn-default" /> |
                        @Html.ActionLink("Quay Lại", "Index")
                    </div>
                }
            </div>
        </div>
    </div>
</div>

